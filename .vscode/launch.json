    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Workspace Startup File",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/example_plot.py"
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}"
        },
        {
            "name": "Python: Terminal (integrated)",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Terminal (external)",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "externalTerminal"
        },
    ]
}